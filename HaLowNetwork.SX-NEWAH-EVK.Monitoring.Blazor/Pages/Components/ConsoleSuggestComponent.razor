<span @onclick="ClickSpan">@Text</span>

@code {

    [Parameter]
    public string Text { get; set; }
    
    [Parameter]
    public string Commnand { get; set; }
    
    [Parameter]
    public EventCallback<string> Click { get; set; }

    private async void ClickSpan()
    {
        await Click.InvokeAsync(Commnand);
    }
    
}

